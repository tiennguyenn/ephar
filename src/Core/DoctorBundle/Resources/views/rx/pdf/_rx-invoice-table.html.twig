<table class="table-subdata table-right">
  <tr>
    <td colspan="2">
      <p class="info-medium text-light-gray">Order Totals:</p>
    </td>
    <td></td>
  </tr>
  <tr>
    <td class="text-gray" width="70%"><strong>Sub Total (Medication):</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ orderTotal.subTotalMedication|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
  </tr>
  {% if rxInformation.hasRxReviewFee %}
  <tr>
    <td class="text-gray" width="70%"><strong>Sub Total (Doctor Fees):</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    {% if platform_share_fee %}
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ (doctorFee.total + doctorFee.adminServiceFee)|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
    {% else %}
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ doctorFee.total|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
    {% endif %}
  </tr>
  {% endif %}
  {% if isInvoicePaid %}
  <tr>
    <td class="text-gray" width="70%"><strong>Shipping and Handling:</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ shippingList|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
  </tr>
  {% if ccAdminFee != 0 %}
  <tr>
    <td class="text-gray" width="70%"><strong>Customs Clearance Admin Fee  - {{ country }}:</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ ccAdminFee|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
  </tr>
  {% endif %}
  {% if igPermitFee != 0 %}
  <tr>
    <td class="text-gray" width="70%"><strong>SG Customs IG Permit Fee:</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ igPermitFee|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
  </tr>
  {% endif %}
  <tr>
    <td class="text-gray" width="70%"><strong>Total:</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ totalBeforeGst|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
  </tr>
  {% if feeGst != 0 and doctorInformation.hasTax %}
  <tr>
    <td class="text-gray" width="70%"><strong>Patient GST %:</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ feeGst|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
  </tr>
  {% endif %}
  {% if importTaxFee != 0 %}
  <tr>
    <td class="text-gray" width="70%"><strong>Import TAX/VAT - {{ country }}:</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%">{% if isCreditNote is defined %}({% endif %}{{ importTaxFee|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</td>
  </tr>
  {% endif %}
  <tr>
    <td class="text-gray" width="70%"><strong>Grand Total:</strong></td>
    <td class="text-gray" width="10%"><strong>{{ currencyCode }}</strong></td>
    <td width="20%"><strong>{% if isCreditNote is defined %}({% endif %}{{ total|number_format(2, '.', ',') }}{% if isCreditNote is defined %}){% endif %}</strong></td>
  </tr>
  {% endif %}
</table>