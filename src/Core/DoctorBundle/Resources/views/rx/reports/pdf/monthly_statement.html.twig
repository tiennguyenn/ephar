                  <td valign="top" class="border-bottom" width="12%"></td>
                  <td valign="top" class="border-bottom" width="15%"></td>
                  <td valign="top" class="border-right border-bottom" width="25%"><strong>Order total</strong></td>
                  <td valign="top" align="right" class="border-right border-bottom {% if col1SubTotal < 0 %}txt-red {% endif %}" width="10%"><strong>{{ col1SubTotal|number_format(2) }}</strong></td>
                  <td valign="top" align="right" class="border-right border-bottom {% if col2SubTotal < 0 %}txt-red {% endif %}" width="10%"><strong>{{ col2SubTotal|number_format(2) }}</strong></td>
                  <td valign="top" align="right" class="border-bottom {% if col3SubTotal < 0 %}txt-red {% endif %}" width="10%"><strong>{{ col3SubTotal|number_format(2) }}</strong></td>
                </tr>
              {% endfor %}
              {% set col1SubtotalAll = col1SubtotalAll + col1SubtotalArea %}
              {% set col2SubtotalAll = col2SubtotalAll + col2SubtotalArea %}
              {% set col3SubtotalAll = col3SubtotalAll + col3SubtotalArea %}
          {% else %}
            <tr><td colspan="7" class="border-bottom">Nil</td></tr>
          {% endif %}
          <tr class="subtotal">
            <td valign="top" class="border-bottom" width="12%"></td>
            <td valign="top" class="border-bottom" width="12%"></td>
            <td valign="top" class="border-bottom" width="15%"></td>
            <td valign="top" class="border-bottom" width="25%"><strong>Subtotal(Adjustments)</strong></td>
            <td valign="top" align="right" class="border-bottom {% if col1SubtotalArea < 0 %}txt-red {% endif %}" width="10%"><strong>{{ col1SubtotalArea |number_format(2)}}</strong></td>
            <td valign="top" align="right" class="border-bottom {% if col2SubtotalArea < 0 %}txt-red {% endif %}" width="10%"><strong>{{ col2SubtotalArea |number_format(2)}}</strong></td>
            <td valign="top" align="right" class="border-bottom {% if col3SubtotalArea < 0 %}txt-red {% endif %}" width="10%"><strong>{{ col3SubtotalArea |number_format(2)}}</strong></td>
          </tr>
          </tbody>
        </table>
      </div>
    </div>
<!-- End adjustments -->

    <div class="content">
      <div class="container">
        <table class="table-data table-multi-line">
          <tbody>
            <tr class="subtotal">
              <td class="border-bottom" width="45%"><strong>{{ paymentInfo.statementTxt }}</strong></td>
              <td class="border-bottom" width="25%"><strong>Grand Total (Local, Overseas Patients, Adjustments)</strong></td>
              <td align="right" class="border-bottom {% if col1SubtotalAll < 0 %} txt-red {% endif %}" width="10%"><strong>{{ col1SubtotalAll|number_format(2, '.', ',')}}</strong></td>
              <td align="right" class="border-bottom {% if col2SubtotalAll < 0 %} txt-red {% endif %}" width="10%"><strong>{{ col2SubtotalAll|number_format(2, '.', ',')}}*</strong></td>
              <td align="right" class="border-bottom {% if col3SubtotalAll < 0 %} txt-red {% endif %}" width="10%"><strong>{{ col3SubtotalAll|number_format(2, '.', ',')}}</strong></td>
            </tr>
          </tbody>
        </table>
       </div>
    </div>

    <div class="content">
      <div class="container">
        <table class="table-data table-multi-line">
          <tbody>
            <tr class="subtotal">
              <td class="border-bottom" width="80%"><strong>{{ paymentInfo.payableTxt }}</strong></td>
              <td align="right" class="border-bottom {% if col3SubtotalAll < 0 %} txt-red {% endif %}" width="20%"><strong>{{ col3SubtotalAll|number_format(2, '.', ',')|save_total_amount}}</strong></td>
            </tr>
          </tbody>
        </table>
       </div>
    </div> 

    <div class="container">
      <p class="align-right mt-15">*See attached invoice</p>
    </div>
  </div>
  <div class="breadcrumb"></div>
</body>
</html>
