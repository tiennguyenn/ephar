
<table class="table table-hover table-light_">
  <thead>
    <tr>
        <th style="width: 20%">Date</th>
        <th style="width: 20%">User</th>
        <th>Changes</th>
    </tr>
  </thead>
  <tbody>
    {% if logs|length > 0 %}
        {% for log in logs %}
            <tr>
                <td>{{ log.createdOn|date('d M y') }}</td>
                <td>{{ log.createdBy }}</td>
                {% if log.title == 'new_delivery_partner_created' %}
                    <td>{{ log.title|replace({'_': ' '})|title }}</td>
                {% else %}
                    <td>
                        {{ item_log(log, 'deliveryName', 'Delivery Partner Name')|raw }}
                        {{ item_log(log, 'businessName', 'Business Registration Number')|raw }}
                        {% if log.oldValue.gst != log.newValue.gst %}
                            GST is updated
                            <br>Old Value: {{ log.oldValue.gst > 0 ? 'Yes' : 'No' }}
                            <br>Old Value: {{ log.newValue.gst > 0 ? 'Yes' : 'No' }}<br><br>
                        {% endif %}
                        {{ item_log(log, 'gstNum', 'GST Registration Number')|raw }}
                        {{ item_log(log, 'email', 'Email')|raw }}
                        {{ item_log(log, 'phoneNumber', 'Mobile Number')|raw }}
                        {{ item_log(log, 'line1', 'Address Line 1')|raw }}
                        {{ item_log(log, 'line2', 'Address Line 2')|raw }}
                        {{ item_log(log, 'line3', 'Address Line 3')|raw }}
                        {{ item_log(log, 'country', 'Country')|raw }}
                        {{ item_log(log, 'state', 'State / Province')|raw }}
                        {{ item_log(log, 'city', 'City')|raw }}
                    </td>
                {% endif %}
            </tr>
        {% endfor %}
    {% else %}
        <tr>
            <td colspan="3"> No log found</td>
        </tr>
    {% endif %}
  </tbody>
</table>
<input type='hidden' id='print_log_url' value="{{ printUrl }}" />
<input type='hidden' id='breadcrumb_title' value="{{ breadcrumbTitle }}" />
<input type='hidden' id='entity_id' value="{{ entityId }}" />
