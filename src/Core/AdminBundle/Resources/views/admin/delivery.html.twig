{% block pageTitle %}
    <h1>List Delivery Partner</h1>
{% endblock %}
{% block breadcrumb %}
    <li> <a href="{{ path('admin_dashboard')}}">Home</a> <i class="fa fa-circle"></i> </li>
    <li> <span class="active">List Delivery Partner </span> </li>
{% endblock %}
{% block content %}
    <div class="portlet light">
        <div class="portlet-title">
            <div class="caption"> <span class="caption-subject font-dark sbold uppercase">List Delivery Partner</span> </div>
        </div>
        <div class="portlet-body">
            <div id="sample_1_wrapper" class="dataTables_wrapper no-footer">
                <div class="row show-grid mb-grid">
                    <div class="col-lg-4 col-md-8">
                        <div id="sample_1_filter">
                            <div class="input-icon right"> <i class="icon-magnifier"></i>
                                <input type="text" class="form-control on-suggestion" placeholder="Search Delivery Partner" id="table-search-data">
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-8 col-md-4">
                        <div class="text-right" id="sample_1_length">
                            <label>
                                <select id="table-length" name="sample_1_length" aria-controls="sample_1" class="form-control input-sm input-xsmall input-inline">
                                    <option value="10">10</option>
                                    <option value="20">20</option>
                                    <option value="50">50</option>
                                </select>
                                entries</label>
                        </div>
                    </div>
                </div>
                <input type='hidden' value="{{ path('admin_delivery_partner_edit',{'id':'id'})}}" id='edit-path' />
                <input type="hidden" value="{{ path('admin_delivery_autocomplete') }}" id="auto-complete-url" />
                <input type="hidden" value="{{ path('admin_delivery_update') }}" id="update-delivery-url" />
                <div class="row show-grid module-filter">                  
                    <div class="col-md-12 text-right align-right">
                        <input type="hidden" id="url_delivery_partner_log" value="{{ path('admin_delivery_partner_log') }}">
                        <a href="{{  path('admin_delivery_partner_create') }}" class="btn btn-ahalf-circle text-uppercase green-seagreen btn-outline btn-icon-right btn-sm ml-10">Add Delivery Partner <i class="fa fa-plus"></i></a>
                    </div>
                </div>
                <div class="table-scrollable">
                    <table class="table table-striped table-bordered table-hover dataTable no-footer dtr-inline" id="sample_1" role="grid" aria-describedby="sample_1_info">
                        <thead>
                            <tr role="row">
                                <th class="sorting" tabindex="0" aria-controls="sample_1" rowspan="1" colspan="1" data-column-index="0" aria-sort="ascending" aria-label=" #Invoice : activate to sort column descending" data-colum-sort="name">Delivery Partners</th>
                                <th class="sorting" tabindex="0" aria-controls="sample_1" rowspan="1" colspan="1" data-column-index="0" aria-sort="ascending" aria-label=" #Invoice : activate to sort column descending" data-colum-sort="registerDate">Register Date</th>
                                <th class="sorting" tabindex="0" aria-controls="sample_1" rowspan="1" colspan="1" data-column-index="0" aria-sort="ascending" aria-label=" #Invoice : activate to sort column descending" data-colum-sort="email">Email Address</th>
                                <th class="sorting" tabindex="0" aria-controls="sample_1" rowspan="1" colspan="1" data-column-index="0" aria-sort="ascending" aria-label=" #Invoice : activate to sort column descending" data-colum-sort="phone">Phone</th>
                                <th class="sorting" tabindex="0" aria-controls="sample_1" rowspan="1" colspan="1" data-column-index="0" aria-sort="ascending" aria-label=" #Invoice : activate to sort column descending" data-colum-sort="country">Country</th>
                                <th class="sorting" tabindex="0" aria-controls="sample_1" rowspan="1" colspan="1" data-column-index="0" aria-sort="ascending" aria-label=" #Invoice : activate to sort column descending" data-colum-sort="state">State</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody id="table-body-delivery">

                        </tbody>
                    </table>
                </div>

                <div class="row">
                    <div class="col-md-5 col-sm-12">
                        <div class="dataTables_info" role="status" aria-live="polite" id="list-tables-info"></div>
                    </div>
                    <div class="col-md-7 col-sm-12">
                        <div class="dataTables_paginate paging_bootstrap_number" id="sample_1_paginate">
                            <ul class="pagination" style="visibility: visible;" id="list-table-pagin">

                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="modal-update-patient-change" class="modal fade modal-new" tabindex="-1" data-width="760">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><i class="fa fa-remove"></i></button>
            <h4 class="modal-title" id="cus-modal-title">Shipping Rate Charged To Patient Change</h4>
        </div>
        <div class="modal-body text-center">
            <form class="form-horizontal">
                <div class="price-changing-square">
                    <div>
                        Old Value: <span class="value" id="origin-value">18.00%</span>          
                    </div>
                    <i class="fa fa-long-arrow-right"></i>
                    <div class="new-value">
                        New Value: <span class="value" id="update-value">20.00%</span>          
                    </div>
                </div>

                <div class="row col-md-offset-2" id="effectDateContainer">
                    <div class="form-group col-md col-md-9">
                        <label for="date-effect" class="col-md-5 control-label control-label-2lines" id='label-for-input' ></label>
                        <div class="col-md-7">
                            <div class="datepicker-wrap">
                                <div class="wrap">
                                    <div class="input-group date date-picker bs-datetime">
                                        <input type="text" size="16" required="required" class="form-control" id="date-effect">
                                        <span class="input-group-addon">
                                            <button class="btn default date-set mr-0" type="button">
                                                <i class="fa fa-calendar"></i>
                                            </button>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <p id="cus-note-mesage">This will recalculate Shipping Rate Charged to Patient for all locations.</p>
                <p>Proceed?</p>

                <a href="javascript:;" class="btn btn-ahalf-circle text-uppercase btn-danger btn-sm" id="btn-cancel-update" >Cancel</a>
                <a href="javascript:;" class="btn btn-ahalf-circle text-uppercase green-seagreen btn-sm ml-15" id="btn-submit-update">Update</a>
            </form>
        </div>
    </div>
