{% javascripts
    '@AdminBundle/Resources/public/js/base.js'
    '@AdminBundle/Resources/public/js/admin_delivery_list.js'
    debug=true

%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
